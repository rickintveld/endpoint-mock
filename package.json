{
    "name": "endpoint-mock",
    "version": "0.0.1",
    "description": "Mock API endpoints based on your input",
    "author": "Rick in 't veld",
    "bin": {
        "api-data-faker": "./bin/run"
    },
    "homepage": "https://github.com/rickintveld/endpoint-mock",
    "license": "MIT",
    "main": "dist/index.js",
    "repository": "https://github.com/rickintveld/endpoint-mock",
    "files": [
        "/bin",
        "/dist"
    ],
    "dependencies": {
        "@oclif/core": "^1",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-plugins": "^2.0.1",
        "axios": "^0.27.2",
        "inquirer": "^8.0.0",
        "prettier": "^2.7.1"
    },
    "devDependencies": {
        "@oclif/test": "^2",
        "@types/chai": "^4",
        "@types/chalk": "^2.2.0",
        "@types/cli-progress": "^3.11.0",
        "@types/inquirer": "^8.2.1",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.9.4",
        "chai": "^4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.2",
        "globby": "^11",
        "mocha": "^9",
        "oclif": "^3",
        "shx": "^0.3.3",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "oclif": {
        "bin": "api-data-faker",
        "dirname": "api-data-faker",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " "
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "engines": {
        "node": ">=12.20.0"
    },
    "bugs": "https://github.com/rickintveld/endpoint-mock/issues",
    "keywords": [
        "fake",
        "data",
        "mock",
        "api",
        "helper"
    ],
    "types": "dist/index.d.ts"
}